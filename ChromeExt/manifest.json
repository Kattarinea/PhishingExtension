{
  "name": "Phishing Scanner",
  "description": "Find phishing URLs",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
	"16": "/icons/ExtIcon16.png",
    "32": "/icons/ExtIcon32.png",
    "48": "/icons/ExtIcon48.png",
	"64": "/icons/ExtIcon64.png",
    "128": "/icons/ExtIcon128.png"
  },
  "permissions": [
	"activeTab", 
	"scripting", 
    "webRequest",
	"storage",
	"tabs"
	],
  "action":{
	"default_popup": "popup.html"
  },
  "background": {
    "service_worker": "service_worker.js"
  }
  
}